    "jest": "^27.3.1",
    "jest-runner-eslint": "^1.0.0",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "braintree": "^3.7.0",
    "connect-flash": "~0.1.1",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "http-errors": "^1.8.0",
    "less-middleware": "^3.1.0",
    "morgan": "^1.10.0",
    "morgan-debug": "^2.0.0",
    "pug": "^3.0.2",
